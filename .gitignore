# Ignore node_modules (si c'est un projet Node.js)
node_modules/

# Ignore build/dist folders
dist/
build/

# Ignore VSCode workspace settings
.vscode/

# Ignore OS-specific files
.DS_Store
Thumbs.db

# Ignore logs
*.log

# Ignore only the contents of eval/ and scores/
eval/*
scores/*

# But keep the directories themselves (so Git tracks empty folders)
!eval/.gitkeep
!scores/.gitkeep
